<p id="notice"><%= notice %></p>

<h5>商务</h5>

<table class="table table-striped">
  <thead class="thead-dark">
    <tr>
      <th scope="col">序</th>
      <th scope="col">编号</th>
      <th scope="col">名称</th>
      <th scope="col">类型</th>
      <!-- <th scope="col">状态</th> -->
      <th scope="col">备注</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @businesses.each do |business| %>
      <tr>
        <th scope="row"><%= link_to business.id,business %></th>
        <td ><%= business.code %></td>
        <td ><%= business.name %></td>
        <td ><%= business.businessflow.name %></td>
        <!-- <td ><%= business.status %></td> -->
        <td ><%= business.remarks %></td>
        <!-- <td><%= link_to 'Show', business %></td>
        <td><%= link_to 'Edit', edit_business_path(business) %></td> -->
        <td><%= link_to 'D', business_path(business), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

